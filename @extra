#include<stdio.h>
#include<conio.h>

 int main()
 {
  int i,j,temp,arr[5];
  clrscr();
  printf("Enter 5 values\n",arr[5]);
  for(i= 0 ; i<5 ; i++){
  scanf("%d", &arr[i]);
  }

  for( i = 0 ; i<5 ; i++){
  for( j = i+1 ; j < 5 ; j++){
  if (arr[i] >arr[j]){
  temp = arr[i] ;
  arr[i] = arr[j] ;
  arr[j] = temp ;
  }
 }
}
	printf("\n number in ascending order");
	for( i = 0; i<5 ; i++){
	 printf("%d",arr[i]);
	 }

   getch();
   return 0;
   }